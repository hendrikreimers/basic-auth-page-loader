{
  "compilerOptions": {
    "target": "es2022",
    "module": "es6",
    "lib": ["es2022", "dom"],
    "strict": true,
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": ["src/**/*"]
}
